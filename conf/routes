GET     /                                       controllers.Application.index()

POST    /api/v1/addCompany                      controllers.CompaniesRestController.create()
GET     /api/v1/companies                       controllers.CompaniesRestController.getAll()
GET     /api/v1/companies/:id                   controllers.CompaniesRestController.getById(id: Long)

GET    /api/v1/identifications                  controllers.IdentificationsRestController.getAll()
GET    /api/v1/identifications/:id              controllers.IdentificationsRestController.getById(id: Long)
POST   /api/v1/startIdentification              controllers.IdentificationsRestController.startIdentification()
GET    /api/v1/pendingIdentifications           controllers.IdentificationsRestController.pendingIdentifications()

GET    /assets/*file                            controllers.Assets.versioned(path="/public", file: Asset)

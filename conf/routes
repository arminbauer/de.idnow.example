GET     /                                       controllers.Application.index()

POST    /api/v1/addCompany                      controllers.CompanyRestController.create()
GET     /api/v1/companies                       controllers.CompanyRestController.getAll()
GET     /api/v1/companies/:id                   controllers.CompanyRestController.getById(id: Long)

GET    /api/v1/identifications                  controllers.IdentificationRestController.getAll()
GET    /api/v1/identifications/:id              controllers.IdentificationRestController.getById(id: Long)
POST   /api/v1/startIdentification              controllers.IdentificationRestController.startIdentification()
GET    /api/v1/pendingIdentifications           controllers.IdentificationRestController.pendingIdentifications()

POST   /ui/startIdentification                  controllers.Application.startIdentification()
POST   /ui/addCompany                           controllers.Application.addCompany()

GET    /assets/*file                            controllers.Assets.versioned(path="/public", file: Asset)

# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                               controllers.Application.main()
POST    /api/v1/addCompany              controllers.CompanyRestController.addCompany()
POST    /api/v1/startIdentification     controllers.IdentificationRestController.startIdentification()
GET     /api/v1/identifications         controllers.IdentificationRestController.identifications()
POST    /mock/startIdentification       controllers.Application.startIdentification()
POST    /mock/addCompany                controllers.Application.addCompany()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
